{
  "extends": ["@libsqlstudio/tsconfig/react"],
  "compilerOptions": {
    "tsBuildInfoFile": ".tsbuildinfo",
    "baseUrl": "./",
    "paths": {
      "@/*": ["./src/*"]
    },
    "noImplicitReturns": false
  },
  "include": ["**/*.ts", "**/*.tsx", ".eslintrc.cjs", "*.js", "*.ts"],
  "exclude": ["node_modules", "dist"]
}
