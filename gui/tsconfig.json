{
  "extends": ["@libsqlstudio/tsconfig/react"],
  "compilerOptions": {
    "tsBuildInfoFile": ".tsbuildinfo",
    "baseUrl": "./",
    "paths": {
      "@gui/*": ["./src/*"]
    },
    "noImplicitReturns": false
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".eslintrc.cjs",
    "*.js",
    "*.ts",
    "jest.config.cjs"
  ],
  "exclude": ["node_modules", "dist"]
}
